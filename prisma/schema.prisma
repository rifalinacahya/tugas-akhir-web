generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pasien {
  id            Int       @id @default(autoincrement())
  nama          String
  umur          Int
  keluhan       String?
  nomorAntrian  Int
  status        String
  tanggalDibuat DateTime @default(now())
}
